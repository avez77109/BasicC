#include <stdio.h>

int main() 
{
   int x,y,z;

   printf ("enter your number x , y , z");
   scanf("%d%d%d" , &x,&y,&z);

if (x>y && x>z)
   {
       printf("x is greter");
   }
else if (y>z)
   {
       printf("y is greater");
   }
else
{
    printf("z is greter");
}

printf("\nthank you");
return 0;
}
