#include <stdio.h>
main()
{int a=13;
 int f=a++;
 
 printf("After Post Increment");
 printf("\n");
 printf(" value of a=%d", a);
 printf("\n");
 printf(" value of f=%d", f);
 int x=--a;
 printf(" Before Pre decrement");
 printf("\n");
 printf(" value of a=%d", a);
 printf("\n");
 printf(" value of x=%d", x);

 
}
    
