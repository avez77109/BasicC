// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a[3][4];
    int i,j;
    printf("enter your no=");
    for(i=0;i<=2;i++)
    {
     for(j=0;j<=2;j++)
     {
         scanf("%d",&a[i][j]);
     }
    }
     for(i=0;i<=2;i++)
    {
     for(j=0;j<=2;j++)
     {
        printf("%d ",a[i][j]);
     }
     printf("\n");
    }
    

    return 0;
}
