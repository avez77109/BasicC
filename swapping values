#include <stdio.h>

int main() {
    int a,b,c;
    printf("enter first number a=");
    scanf("%d", &a);
    printf("enter second number b=");
    scanf("%d", &b);
    printf("swapping details\n");
    printf("------\n");
    printf("previous swapping a=%d\n" ,a);
    printf("previous swapping b=%d\n" ,b);
    c=a;
    a=b;
    b=c;
    printf("------\n");
    printf("after swapping a=%d\n" ,a);
    printf("after swapping b=%d" ,b);

    return 0;
}
