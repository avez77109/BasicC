//Online C compiler to run C program online
#include <stdio.h>
struct employee
{
     char name[20];
     int age;
     char street[20];
     char city[20];
}s[3];
void main()
{
    int i;
    for(i=0;i<3;i++)
    {
        printf("enter name,age,street&city=");
        scanf("%s%d%s%s",s[i].name,&s[i].age,s[i].street,s[i].city);
    }
    
    for(i=0;i<3;i++)
    {
    printf("-------details of employee-------\n");
    printf("name is %s\n",s[i].name);
    printf("age is %d\n",s[i].age);
    printf("street is %s\n",s[i].street);
    printf("city is %s\n",s[i].city);
    }
}

    
