// Online C compiler to run C program online
#include <stdio.h>

void swap(int x,int y)
{
    int z;
    z=x;
    x=y;
    y=z;
    printf("after swapping\n");
    printf("value of a=%d\n",x);
    printf("value of b=%d",y);
}
int main()
{
    int a,b;
    printf("enter a=");
    scanf("%d",&a);
    printf("enter b=");
    scanf("%d",&b);
    printf("before swapping\n");
    printf("value of a=%d\n",a);
    printf("value of b=%d\n",b);
    swap(a,b);
}
